<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">叫车服务</view>
</cu-custom>
<view class="padding-xl radius shadow bg-white">
  <map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="14" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 800rpx;">
  </map>
</view>

<view class="padding-xl radius shadow bg-white">
  <view class="bg-yellow padding  text-center">
    <button class="cu-btn bg-yellow lg" open-type='{{openSetting}}' bindtap='callCar'>一键出行</button>
  </view>
</view>